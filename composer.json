{
    "name": "heinrichschiller/slim4-mustache-view",
    "description": "Slim4-Mustache-View",
    "keywords": ["slim","framework","view","template","mustache"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Heinrich Schiller",
            "email": "info@heinrich-schiller.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3",
        "mustache/mustache": "2.13.0",
        "monolog/monolog": "2.2.0"
    },
    "require-dev": {
        "slim/slim": "4.*",
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Slim\\View\\": "src/View/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpunit": "phpunit"
    }
}
